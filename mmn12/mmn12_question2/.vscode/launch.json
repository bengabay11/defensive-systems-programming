{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run mmn12-q2.cpp - Release",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/dist/release/mmn12-q2",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build mmn12-q2.cpp (Release)",
			"windows": {
				"miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe"
			},
			"linux": {
				"miDebuggerPath": "/usr/bin/gdb"
			}
		},
